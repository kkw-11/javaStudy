package javaStudy;

public class PrimitiveDataTypeExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		boolean isFun = true;
		
		System.out.println(isFun);
		
		
		char a = 65;
		char b = 'A';
		System.out.println(a);
		System.out.println(b);
		
		
		int x = 45;
		
		long big = 32321312312l;

	}

}
